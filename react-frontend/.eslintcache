response = (response) => {\n      setLikeList(response.data.like);\n    };\n\n    axios_request({\n      route: route,\n      method: method,\n      data: \"\",\n      axios_error: cb_error,\n      axios_response: cb_response,\n    });\n  };\n\n  const get_friend_list = () => {\n    const route = `/post/${postid}/like`;\n    const method = \"GET\";\n    const cb_error = (err) => {\n      setError(err.message);\n    };\n\n    const cb_response = (response) => {\n      setMyFriendList(response.data.like);\n    };\n\n    axios_request({\n      route: route,\n      method: method,\n      data: \"\",\n      axios_error: cb_error,\n      axios_response: cb_response,\n    });\n  };\n\n  useEffect(() => {\n    get_users_liked();\n    // get_friend_list();\n  }, []);\n\n  return (\n    <div className=\"UserLikes\">\n      <div className=\"box\">\n        <div\n          className=\"form-close-btn fas fa-times-circle\"\n          onClick={(e) => {\n            e.preventDefault();\n            e.stopPropagation();\n            setUsersLikedIndex(null);\n          }}\n        ></div>\n        {likeList.map((value, index) => {\n          return (\n            <UserLikesCard\n              value={value}\n              index={index}\n              key={uniqid()}\n              myFriendList={myFriendList}\n            />\n          );\n        })}\n      </div>\n    </div>\n  );\n};\n\nexport default UserLikes;\n","/home/arvind/Documents/React-apps/odin-book/react-frontend/src/Components/UserDetail/UserLikesCard.js",[],"/home/arvind/Documents/React-apps/odin-book/react-frontend/src/Components/Comment/CommentCreate.js",[],"/home/arvind/Documents/React-apps/odin-book/react-frontend/src/Components/Comment/CommentUpdate.js",[],"/home/arvind/Documents/React-apps/odin-book/react-frontend/src/Components/Comment/CommentForm.js",["366"],"/home/arvind/Documents/React-apps/odin-book/react-frontend/src/Components/Comment/CommentDelete.js",["367","368","369","370"],"import React, { useContext, 